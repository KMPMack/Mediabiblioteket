package mediabiblioteket;

import static com.oracle.nio.BufferSecrets.instance;
import org.junit.After;
import org.junit.AfterClass;
import org.junit.Before;
import org.junit.BeforeClass;
import org.junit.Test;
import static org.junit.Assert.*;

/**
 *
 * @author NKPGV
 */
public class BorrowerTest {

    public BorrowerTest() {
    }

    @BeforeClass
    public static void setUpClass() {
    }

    @AfterClass
    public static void tearDownClass() {
    }

    @Before
    public void setUp() {
    }

    @After
    public void tearDown() {
    }

    /**
     * Test of setName method, of class Borrower.
     */
    @Test
    public void testGetName() {
        String testdata = "Mack";
        Borrower name = new Borrower("Mack", "", "");
        assertEquals(name.getName(), testdata);
    }

    @Test
    public void testSetName() {
        String name = "Mack";
        Borrower test1 = new Borrower("","","");
       
        test1.setName(name);
        String Result = test1.getName();
        String expResult = "Mack";
        assertEquals(expResult, Result);
      
    }

    @Test
    public void testGetSsn() {
        String testdata = "123";
        Borrower Ssn = new Borrower(" ", testdata, " ");
        assertEquals(Ssn.getSsn(), testdata);
    }

    @Test
    public void testSetSsn() {
        String Ssn = "123";
        Borrower test2 = new Borrower("", "", "");
        test2.setSsn(Ssn);
        String Result = test2.getSsn();
        String expResult = "123";
        assertEquals(expResult, Result);
    }

    @Test
    public void testGetPhoneNumber() {
        String testdata = "0723411888";
        Borrower phoneNumber = new Borrower(" ", " ", testdata);
        assertEquals(phoneNumber.getphoneNumber(), testdata);

    }

    @Test
    public void testSetPhoneNumber() {
        String PhoneNumber = "0723411888";
        Borrower test3 = new Borrower("", "", "");
        test3.setphoneNumber(PhoneNumber);
        String Result = test3.getphoneNumber();
        String expResult ="0723411888";
        assertEquals(expResult, Result);
     
    }

}
